{
  "compilerOptions": {
    "outDir": "../../dist/",
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es2015",
    "allowJs": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "removeComments": true,
    "strict": true,
    "noUnusedLocals": true,
    "strictPropertyInitialization": true,
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "lib": [
      "es2015",
      "dom"
    ]
  }
}